{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", ".webpack/**", "out/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "lint:ci": {
      "dependsOn": ["^build"]
    },
    "check:types": {
      "dependsOn": ["^build"]
    }
  }
}
