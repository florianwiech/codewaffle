{
  "name": "codewaffle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "format:check": "prettier 'src/**/*' --check --ignore-unknown",
    "format:write": "prettier 'src/**/*' --write --ignore-unknown",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --format friendly src/",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --format friendly --fix src/",
    "dep:check": "npx npm-check-updates"
  },
  "dependencies": {
    "@codemirror/autocomplete": "0.19.8",
    "@codemirror/closebrackets": "0.19.0",
    "@codemirror/commands": "0.19.5",
    "@codemirror/comment": "0.19.0",
    "@codemirror/fold": "0.19.1",
    "@codemirror/gutter": "0.19.5",
    "@codemirror/highlight": "0.19.6",
    "@codemirror/history": "0.19.0",
    "@codemirror/lang-javascript": "0.19.3",
    "@codemirror/language": "0.19.5",
    "@codemirror/language-data": "0.19.1",
    "@codemirror/lint": "0.19.3",
    "@codemirror/matchbrackets": "0.19.3",
    "@codemirror/rectangular-selection": "0.19.1",
    "@codemirror/search": "0.19.2",
    "@codemirror/state": "0.19.6",
    "@codemirror/view": "0.19.20",
    "@primer/primitives": "7.1.0",
    "fuse.js": "6.4.6",
    "jwt-decode": "^3.1.2",
    "lodash.debounce": "4.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "rxjs": "7.4.0",
    "styled-components": "5.3.3",
    "ua-parser-js": "1.0.2",
    "web-vitals": "2.1.2"
  },
  "devDependencies": {
    "@testing-library/dom": "8.11.1",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.3",
    "@types/lodash.debounce": "4.0.6",
    "@types/node": "16.11.9",
    "@types/react": "17.0.35",
    "@types/react-dom": "17.0.11",
    "@types/styled-components": "5.1.15",
    "@types/ua-parser-js": "0.7.36",
    "eslint-config-prettier": "8.3.0",
    "eslint-formatter-friendly": "7.0.0",
    "prettier": "2.4.1",
    "react-scripts": "4.0.3",
    "typescript": "4.5.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "rules": {
      "import/order": "warn",
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
