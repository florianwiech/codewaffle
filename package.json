    "@codemirror/state": "^0.19.4",
    "@codemirror/view": "^0.19.13",
    "@primer/primitives": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^7.4.0",
    "styled-components": "^5.3.3",
    "ua-parser-js": "^1.0.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
