{
  "private": true,
  "name": "@codewaffle/components",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build": "yarn clean && codewaffle-scripts build",
    "build:storybook": "build-storybook",
    "test": "codewaffle-scripts test",
    "lint": "eslint src --format friendly",
    "lint:stage": "eslint src --format friendly --max-warnings=0",
    "lint:fix": "eslint src --format friendly --fix",
    "check:types": "tsc --noEmit"
  },
  "dependencies": {
    "@primer/primitives": "^7.4.1",
    "bootstrap-icons": "^1.8.0",
    "fuse.js": "^6.5.3"
  },
  "peerDependencies": {
    "react": ">= 17",
    "react-dom": ">= 17",
    "rxjs": ">= 7",
    "styled-components": ">= 5"
  },
  "devDependencies": {
    "@codewaffle/buildhelper": "*",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@testing-library/dom": "8.11.3",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "eslint": "^8.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "storybook-dark-mode": "^1.0.8",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.5"
  }
}
